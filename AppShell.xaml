<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RentACar_Mobile_.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RentACar_Mobile_"
    Shell.FlyoutBehavior="Disabled">
    <TabBar>
       
       
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}" />

        <ShellContent
            Title="Start"
            ContentTemplate="{DataTemplate local:StartPage}" />

        <ShellContent
            Title="My Cars"
            ContentTemplate="{DataTemplate local:CarEntryPage}" />

        <ShellContent
             Title="My Dealerships"
             ContentTemplate="{DataTemplate local:DealershipEntryPage}" />

        <ShellContent
 Title=" Our Luxury Collections"
 ContentTemplate="{DataTemplate local:CollectionEntryPage}"
 />

    </TabBar>
</Shell>
